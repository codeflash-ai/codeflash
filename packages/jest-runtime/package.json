{
  "name": "@codeflash/jest-runtime",
  "version": "0.1.0",
  "description": "Codeflash Jest runtime helpers for test instrumentation and behavior verification",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "codeflash-compare": "./bin/compare.js"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./serializer": {
      "require": "./src/serializer.js",
      "import": "./src/serializer.mjs"
    },
    "./comparator": {
      "require": "./src/comparator.js",
      "import": "./src/comparator.mjs"
    },
    "./capture": {
      "require": "./src/capture.js",
      "import": "./src/capture.mjs"
    }
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "src/",
    "bin/"
  ],
  "scripts": {
    "test": "jest",
    "build": "echo 'No build step required'",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "codeflash",
    "jest",
    "testing",
    "optimization",
    "instrumentation",
    "serialization"
  ],
  "author": "Codeflash AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codeflash-ai/codeflash.git",
    "directory": "packages/jest-runtime"
  },
  "bugs": {
    "url": "https://github.com/codeflash-ai/codeflash/issues"
  },
  "homepage": "https://codeflash.ai",
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "jest": ">=27.0.0"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.0.0",
    "@msgpack/msgpack": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
